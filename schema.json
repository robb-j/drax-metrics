{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$comment": "This is a sample schema for Drax metrics",
  "anyOf": [
    {
      "type": "object",
      "properties": {
        "name": { "type": "string", "const": "page_view" },
        "url": { "type": "string" }
      },
      "additionalProperties": false,
      "required": ["name", "url"]
    },
    {
      "type": "object",
      "properties": {
        "name": { "type": "string", "const": "login" }
      },
      "additionalProperties": false,
      "required": ["name"]
    },
    {
      "type": "object",
      "properties": {
        "name": { "type": "string", "const": "logout" }
      },
      "additionalProperties": false,
      "required": ["name"]
    }
  ]
}
